{
    "name": "parenthesis/dingconnect-api-php",
    "description": "A PHP Client for the  DingConnect Mobile Top-Up API.",
    "type": "library",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api",
        "dingconnect"
    ],
    "homepage": "https://github.com/parenthesislab/dingconnect-api-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Parenthesis Tech LLC",
            "email": "hi@parenthesis.io",
            "homepage": "https://parenthesis.io",
            "role": "Company"
        },
        {
            "name": "Elvis Morales",
            "email": "yo@elvismdev.io",
            "homepage": "https://elvismdev.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "Parenthesis\\DingConnect\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Parenthesis\\DingConnect\\" : "test/" }
    }
}
